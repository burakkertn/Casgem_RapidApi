@model List<ExchaneViewModel.Exchange_Rates>
@{
    Layout = null;
}
<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
<div class="container">
    <br /><br />
    <table class="table table-hover">

        <tr>

            <th>#</th>
            <th> #</th>
            <th># </th>
           
        </tr>
        @foreach (var item in Model)
        {
            <tr>

                <td></td>
                <td>@item.currency</td>
                <td>@item.exchange_rate_buy</td>
             
            </tr>
        }


    </table>
</div>